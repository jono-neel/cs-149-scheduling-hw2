import java.nio.channels.Pipe;
import java.util.ArrayList;

/**
* Hmmm, issues ending the Threads.
* Not much I did here but its a start?
*
*/
public class Process 
{
	final int END_TIME = 30;
	ChildProcess[] childList;
	Thread[] childThread;
	Pipe[] childPipe;
	long startTime;
	
	public Process()
	{
		childList = new ChildProcess[5];
		childThread = new Thread[5];
		childPipe = new Pipe[5];
	}
	public void start()
	{
		startTime = System.currentTimeMillis();
		for(int i = 0; i < 5; i++)
		{
			childList[i] = new ChildProcess(i,startTime);
			childThread[i] = new Thread(childList[i]);
			childPipe[i] = childList[i].getPipe();
		}
		for(int i = 0; i < 5; i++)
		{
			childThread[i].start();
		}
	}
}
